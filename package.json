{
  "name": "react-flight-tracker",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@react-three/drei": "^9.78.2",
    "@react-three/fiber": "^8.13.5",
    "@react-three/postprocessing": "^2.14.13",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "@types/three": "^0.154.0",
    "axios": "^1.4.0",
    "flightradar24-client-ts": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "three": "^0.154.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "marked-gfm-heading-id": "^3.0.4",
    "marked-mangle": "^1.1.0",
    "semantic-release": "^21.0.7",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "verifyReleaseCmd": "echo ${nextRelease.version} > .VERSION"
        }
      ],
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            },
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "ci",
              "scope": "release",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false,
                "release": "patch"
              },
              {
                "type": "chore",
                "section": "Other Updates",
                "hidden": false,
                "release": "patch"
              },
              {
                "type": "docs",
                "section": "Documentation",
                "hidden": false,
                "release": "patch"
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/gitlab",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "yarn.lock",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "license": "MIT",
  "author": "Appadoo Apoorva Srinivas",
  "homepage": "https://gitlab.com/dev6645326/react-flight-tracker",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dev6645326/react-flight-tracker.git"
  },
  "bugs": {
    "url": "https://gitlab.com/dev6645326/react-flight-tracker/-/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-three-fiber",
    "threejs",
    "typescript",
    "vite"
  ]
}
